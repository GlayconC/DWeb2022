<?php
class a {
  
    public function Html() {
        echo "<!DOCTYPE html>";
    }

    public function Html2() {
        echo "<html " .$this->lang." >";
    }

    public function Head() {
        echo "<head>";
    }

    public function Meta() {
        echo $this->meta1;
    }

    public function Title() {
        echo "<title>Document</title>";
    }

    public function Body() {
        echo "<body>";
    }
}

$html1 = new a();
$html1->Html();

$html21 = new a();
$html21->lang = "lang='en'";
$html21->Html2();

$head = new a();
$head->Head();

$ameta = new a();
$ameta->meta1 = "<meta charset='UTF-8'>";
$ameta->Meta();

$bmeta = new a();
$bmeta->meta1 = "<meta http-equiv='X-UA-Compatible' content='IE=edge'>";
$bmeta->Meta();

$cmeta = new a();
$cmeta->meta1 = "<meta name='viewport' content='width=device-width, initial-scale=1.0'>";
$cmeta->Meta();

$title1 = new a();
$title1->Title();

$head1 = new a();
$head1->Head();

for ($i=0; $i < 2; $i++) {
    $body= new a();
    $body->Body();

}    

$html1 = new a();
$html1->Html();

?>
